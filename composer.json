{
    "name": "sdv/laravel-endpoint",
    "description": "Laravel Endpoint is a CRUD REST API package for Laravel.",
    "keywords": [
        "laravel",
        "api",
        "rest",
        "search",
        "scaffolding"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Fabrice Clementz",
            "email": "f.clementz@sdv.fr"
        }
    ],
    "autoload": {
        "psr-4": {
            "SdV\\Endpoint\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SdV\\Endpoint\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/database": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0",
        "spatie/laravel-fractal": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.0.0",
        "orchestra/testbench": "^8.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SdV\\Endpoint\\EndpointServiceProvider"
            ]
        }
    }
}
